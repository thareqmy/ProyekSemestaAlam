<% flash.each do |name, msg| %>
  <div align = "center" >
    <div class="alert alert-warning alert-dismissible fade show col-xl-10" role="alert">
      <%= msg %>
      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
    </div>
  </div>
<% end %>